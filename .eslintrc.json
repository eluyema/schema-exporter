{
  "extends": ["plugin:sonarjs/recommended", "metarhia","prettier"],
  "plugins": ["sonarjs","prettier"],
  "rules": {
    "sonarjs/cognitive-complexity": "error",
    "sonarjs/no-identical-expressions": "error",
    "strict":"off",
    "prettier/prettier": ["error",{
      
    }],
    "linebreak-style": "off",
    "quotes":["warn","double"]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}